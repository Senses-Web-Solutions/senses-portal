{
    "id": "senses-markers-label",
    "type": "symbol",
    "source": "markers",
    "minzoom": 8,
    "filter": ["!", ["has", "point_count"]],
    "layout": {
        "text-field": "{label}",
        "text-font": ["Noto Sans Regular"],
        "text-size": 10,
        "text-letter-spacing": 0.05,
        "text-offset": [0, 1.5]
    },
    "paint": {
        "text-color": "#000",
        "text-halo-color": "#fff",
        "text-halo-width": 1
    }
}
